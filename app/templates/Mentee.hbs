
<div class="contact-head" style="overflow: hidden;">
  <div class="bar">
    <!--Profile Pic-->
    {{#if avatar_mobile_url}}
      <img class="profile-pic" data-src="mentee.avatar_mobile_url" src={{avatar_mobile_url}}>
    {{else}}
      <i class="icon-user" style="display: inline-block"></i>
    {{/if}}
    <span class="name" data-text="mentee.name">{{name}}</span>
    <a class="details pull-right">
      <i class="icon-edit"></i>
    </a>
  </div>
  <br>
  <div id="edit-mentee"></div>
</div>

<div id="mentee-action-grid" class="container">
  <div class="row">
    <div class="col col-1">
      <div id="snapshotBtn"></div>
    </div>
  </div>
  <div class="row">
    <div class="col col-1">
      <div id="selfassessBtn"></div>
    </div>
  </div>
  <div class="row">
    <div class="col col-1">
      <div id="interviewBtn"></div>
    </div>
  </div>
  <div class="row">
    <div class="col col-1">
      <div id="observationsBtn"></div>
    </div>
  </div>
  <div class="row">
    <div class="col col-1">
      <div id="developBtn"></div>
    </div>
  </div>
  <div class="row">
    <div class="col col-1">
      <div id="lifelistBtn"></div>
    </div>
  </div>
  <div class="row">
    <div class="col col-1">
      <div id="planBtn"></div>
    </div>
  </div>
  <div class="row">
    <div class="col col-1">
      <div id="journalBtn"></div>
    </div>
  </div>
  <div class="row">
    <div class="col col-1">
      <div id="contactsBtn"></div>
    </div>
  </div>
</div>